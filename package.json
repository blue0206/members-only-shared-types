{
    "name": "@blue0206/members-only-shared-types",
    "version": "12.0.0",
    "description": "Shared TypeScript types and schemas for Members Only project.",
    "keywords": [
        "typescript",
        "shared",
        "zod",
        "dto",
        "members-only"
    ],
    "homepage": "https://github.com/blue0206/members-only-shared-types#readme",
    "bugs": {
        "url": "https://github.com/blue0206/members-only-shared-types/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/blue0206/members-only-shared-types.git"
    },
    "license": "ISC",
    "author": "Aayush Rai",
    "type": "module",
    "sideEffects": false,
    "exports": {
        "./api": {
            "import": "./dist/api/base.js",
            "module-sync": "./dist/api/base.js",
            "types": "./dist/api/base.d.ts"
        },
        "./constants/error": {
            "import": "./dist/api/error-codes.js",
            "module-sync": "./dist/api/error-codes.js",
            "types": "./dist/api/error-codes.d.ts"
        },
        "./constants/event": {
            "import": "./dist/api/event-names.js",
            "module-sync": "./dist/api/event-names.js",
            "types": "./dist/api/event-names.d.ts"
        },
        "./dtos/*": {
            "import": "./dist/dtos/*.js",
            "module-sync": "./dist/dtos/*.js",
            "types": "./dist/dtos/*.d.ts"
        },
        "./enums/role": {
            "import": "./dist/enums/roles.enum.js",
            "module-sync": "./dist/enums/roles.enum.js",
            "types": "./dist/enums/roles.enum.d.ts"
        },
        "./enums/event": {
            "import": "./dist/enums/eventReason.enum.js",
            "module-sync": "./dist/enums/eventReason.enum.js",
            "types": "./dist/enums/eventReason.enum.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "prepublishOnly": "npm run build",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix"
    },
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@eslint/js": "^9.24.0",
        "eslint": "^9.24.0",
        "eslint-config-prettier": "^10.1.2",
        "eslint-plugin-markdown": "^5.1.0",
        "eslint-plugin-prettier": "^5.2.6",
        "prettier": "3.5.3",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.29.1"
    },
    "dependencies": {
        "zod": "^3.24.3"
    }
}
